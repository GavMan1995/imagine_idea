<div class="container-fluid">
  <div class="row landing-row">
    <img style="max-height: 800px;" class="col-md-6 center-block"src="http://i.imgur.com/nUtesBv.png" />
    <div class="container">
      <h3 class="col-md-6 imagine-idea-tag">IMAGINE IDEA: Dreams <i class="fa fa-arrow-circle-o-right" aria-hidden="true"></i>
 Reality</h3>
      <form>
        <div class="form-group col-md-6">
          <input type="text" class="form-control" id="first_name_id" placeholder="First Name" autofocus="true">
        </div>
        <div class="form-group col-md-6">
          <input type="text" class="form-control" id="last_name_id" placeholder="Last Name">
        </div>
        <div class="form-group col-md-6">
          <input type="text" class="form-control" id="idea_name_id" placeholder="Idea Name">
        </div>
        <div class="form-group col-md-6">
          <textarea class="form-control" rows="4" id="idea_description_id" placeholder="Idea Description"></textarea> 
        </div>
        <div class="form-group col-md-6">
          <input type="email" class="form-control" id="email_id" placeholder="Email">
        </div>
        <div class="form-group col-md-6">
          <input type="number" class="form-control" id="phone_number_id" placeholder="Phone Number">
        </div>
        <div class="form-group col-md-6">
          <button type="submit" class="btn btn-success btn-large btn-block">Create My Idea</button>
        </div>
      </form>
    </div>
  </div>
</div>